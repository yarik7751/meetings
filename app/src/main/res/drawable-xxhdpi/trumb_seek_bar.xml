<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@android:color/transparent" />
            <size
                android:width="100dp"
                android:height="20dp" />
        </shape>
    </item>
    <item
        android:left="40dp"
        android:right="40dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/seek_bar" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
</layer-list>